<% title "Instruments" %>

<table>
  <% for instrument in @instruments %>
    <tr>
      <td><%=h instrument.user_id %></td>
      <td><%= link_to "Show", instrument %></td>
      <td><%= link_to "Edit", edit_instrument_taggings_url(:instance => instrument) %></td>
      <td><%= link_to "Destroy", instrument, :confirm => 'Are you sure?', :method => :delete %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Instrument", new_instrument_path %></p>
